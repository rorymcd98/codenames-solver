<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>codenames_solver</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Shared\Button.razor.css" />
    <None Remove="Shared\SimilarityItemsSidebar.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Shared\Button.razor.css" />
    <Content Include="Shared\SimilarityItemsSidebar.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FuzzySharp" Version="2.0.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Word2vec.Tools" Version="2.1.0" />
  </ItemGroup>

</Project>
